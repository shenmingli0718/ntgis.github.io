  {
    "$schema": "https://railway.app/railway.schema.json",
    "services": {
        "python-service": {
            "build": {
                "builder": "NIXPACKS"
            },
            "deploy": {
                "startCommand": "python -m pip install -r requirements.txt && gunicorn int_gis_use_dash:server --bind 0.0.0.0:8799 --workers=4",
                "restartPolicyType": "ON_FAILURE",
                "restartPolicyMaxRetries": 5
            }
        },
        "node-service": {
            "build": {
                "builder": "NIXPACKS"
            },
            "deploy": {
                "startCommand": "npm install && npm start",
                "restartPolicyType": "ON_FAILURE",
                "restartPolicyMaxRetries": 5
            }
        }
    }
  }

